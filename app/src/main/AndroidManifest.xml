<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.biosym.jobmatching">

    <application
        android:name=".AmplifyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JobMatching">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" android:screenOrientation="landscape"/>
        <activity android:name=".CheckImageActivity" android:screenOrientation="landscape"/>
        <activity android:name=".JoinActivity" android:screenOrientation="landscape"/>
        <activity android:name=".EmailConfirmationActivity" android:screenOrientation="landscape"/>
        <activity android:name=".LoginActivity" android:screenOrientation="landscape"/>
    </application>

</manifest>